.bottombox{
  width: 98%;
  margin:0 auto;
  height:50%;
  background-color: white;
  border-radius: 10px;
  font-size: 13px;
}
.navbar{
  padding-top: 5px;
  flex: none;
  width: 100%;
  display: flex;
  height: 40px;
  line-height: 40px;
  text-align: center;
  justify-content: center;
  background:white;
  text-align: center;
  font-size: 16px;
  /* border-bottom: #cecece 3rpx solid; */
}
.item.active{
  border-radius: 18px;
  width: 90px;
  height: 40px;
  line-height: 40px;
  text-align: center;
}
.navbar .activeL{
  width: 110px;
  height: 35px;
  line-height: 35px;
  text-align: center;
  color: white;
  background:#fb9527 ;
  border-radius: 25px 0 0 25px ;
  border: #fb9527 2px solid;
  /* background-size: 100px 40px; */
}
.navbar .activeR{
  width: 110px;
  height: 35px;
  line-height: 35px;
  text-align: center;
  color: white;
  background:#fb9527 ;
  border-radius:0 25px 25px 0;
  border: #fb9527 2px solid;

  /* background-size: 100px 40px; */
}
