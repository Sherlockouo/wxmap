@import '/weui-wxss-master/dist/style/weui.wxss';

.titleCss{
  width: 100%;
  height: 30vh;
  text-align: center;
}
.mbody
{
  margin-top: 15%;
}
.Bac{
  position: fixed ;
  z-index: -10 ;
}
.setBac{
  display: flex;
  flex-direction:column;
  align-items:center;
  justify-content: space-between;
  padding-top: 20px;
}
.headimg{
  width: 80px;
  height: 80px;
}
.box{
  margin-top: 30px;
}
.head{
  width: 80px;
  height: 80px;
  border-radius: 40px;
  background: #a5a5a5;
}
.btn{
  background-color: #ffffff;
  width: 120px;
  height: 35px;
  line-height: 35px;
  text-align: center;
  border-radius: 40px;
}
.addBnCss{
  margin-top:5vh;
}
.iconCss{
  width: 3.5vh;
  height: 3.5vh;
}
.labelCss{
  display: flex;
}
.textCss{
  font-size: 24rpx;
  margin-left: 5vh;
  line-height: 3.5vh;
}
.inputCss{
  margin-left: 2vh;
}
.place-holder{
  font-size: 28rpx;
}
.weui-cell{
  height: 40px;
}
